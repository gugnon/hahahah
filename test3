-- List of valid keys
local validKeys = {
    "key1",
    "key2",
    "key3",
    "key4"
}

-- Function to check if the input key is valid
local function isValidKey(inputKey)
    for _, key in ipairs(validKeys) do
        if key == inputKey then
            return true
        end
    end
    return false
end

-- Prompt the user to enter a key
print("Please enter your key:")
local userInputKey = io.read()

-- Validate the key
if isValidKey(userInputKey) then
    print("Key is valid. Loading script...")

    -- Loading the actual script
    -- Example: loadstring(game:HttpGet("https://your-script-url.com"))()
else
    -- Kick the player if the key is invalid
    local Players = game:GetService("Players")
    local player = Players.LocalPlayer
    player:Kick("Invalid key. Access denied.")
end
